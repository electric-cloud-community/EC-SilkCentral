<project name="EC-SilkCentral" default="package" basedir=".">
	<description>
        Build the EC-SilkCentral plugin
    </description>

    <property name="buildconfigdir" value="../Plugin-In-A-Box/scripts" />
    
	<!-- plugin specific properties -->
	<property name="pluginKey" value="EC-SilkCentral" />
	<property name="pluginVersion" value="1.0" />
	<property name="gwtModules" value="ecplugins.silkCentral.ConfigurationManagement" />
    <property name="package.export.src" value="silkCentral.srcs"/>

    <import file="${buildconfigdir}/buildTargets.xml"/>

    <!-- Package sources for exported utility classes. -->
    <fileset id="silkCentral.srcs" dir="${dir.src}">
      <include name="**/PluginConstants.java"/>
      <include name="**/SilkCentralConfigList.java"/>
      <include name="**/SilkCentralConfigListLoader.*"/>
    </fileset>
	
	
	<fileset id="images" dir="${dir.htdocs}">
      <include name="**/PluginConstants.java"/>
      <include name="**/SilkCentralConfigList.java"/>
      <include name="**/SilkCentralConfigListLoader.*"/>
    </fileset>
	
	
	
</project>
